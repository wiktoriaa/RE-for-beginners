loc_80483D8:
; EBX pointe sur la structure, ESI pointe sur la fin de celle-ci.
                mov     eax, [ebx]	; get 32-bit word from array
                add     ebx, 4          ; prochain champ de la structure
                mov     dword ptr [esp+4], offset a0x08xD ; "0x%08X (%d)\n"
