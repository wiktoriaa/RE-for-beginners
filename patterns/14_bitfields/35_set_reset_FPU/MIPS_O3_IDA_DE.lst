my_abs:
; hole 1 vom Koprozessor:
                mfc1    $v1, $f12
                li      $v0, 0x7FFFFFFF
; $v0=0x7FFFFFFF
; führe AND aus:
                and     $v0, $v1
; kopiere 1 zum Koprozessor 1:
                mtc1    $v0, $f0
; return
                jr      $ra
                or      $at, $zero ; branch delay slot

set_sign:
; hole 1 vom Koprozessor:
                mfc1    $v0, $f12
                lui     $v1, 0x8000
; $v1=0x80000000
; führe OR aus:
                or      $v0, $v1, $v0
; kopiere 1 zum Koprozessor:
                mtc1    $v0, $f0
; return
                jr      $ra
                or      $at, $zero ; branch delay slot

negate:
; hole 1 vom Koprozessor:
                mfc1    $v0, $f12
                lui     $v1, 0x8000
; $v1=0x80000000
; do XOR:
                xor     $v0, $v1, $v0
; kopiere 1 zum Koprozessor:
                mtc1    $v0, $f0
; return
                jr      $ra
                or      $at, $zero ; branch delay slot
